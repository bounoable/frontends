{
  "name": "@shopware-pwa/nuxt3-module",
  "version": "1.0.2",
  "description": "Nuxt 3 module for Shopware Frontends",
  "author": "Shopware",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shopware/frontends.git"
  },
  "homepage": "https://frontends.shopware.com/framework/internal-structure.html#nuxt3-module",
  "keywords": [
    "Nuxt",
    "Shopware",
    "Vue"
  ],
  "bugs": {
    "url": "https://github.com/shopware/frontends/issues"
  },
  "license": "MIT",
  "main": "./index.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "exports": {
    ".": {
      "types": "./dist/module.d.ts",
      "import": "./dist/module.mjs",
      "require": "./index.cjs"
    }
  },
  "files": [
    "dist",
    "plugin.ts",
    "shopware.d.ts",
    "index.cjs"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint src/**/*.ts* --fix --max-warnings=0 && pnpm run typecheck",
    "typecheck": "tsc --noEmit",
    "test": "echo \"Warn: no test specified yet\""
  },
  "dependencies": {
    "@nuxt/kit": "3.12.4",
    "@shopware-pwa/composables-next": "workspace:*",
    "@shopware-pwa/helpers-next": "workspace:*",
    "@shopware/api-client": "workspace:*",
    "defu": "6.1.4",
    "js-cookie": "3.0.5"
  },
  "devDependencies": {
    "@nuxt/schema": "3.12.4",
    "eslint-config-shopware": "workspace:*",
    "@nuxt/module-builder": "^0.8.1",
    "nuxt": "3.12.4",
    "tsconfig": "workspace:*",
    "typescript": "5.5.4",
    "unbuild": "2.0.0"
  }
}
